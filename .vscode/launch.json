    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Tic Tac Toe",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\TicTacToe.js",
            "env": {
                "NODE_ENV":"development",
                "port":"8080",
                "uri" : "mongodb://127.0.0.1:27017",
                "dbname":"TicTacToe",
                "collection":"gameUsers"
            }
        }
    ]
}